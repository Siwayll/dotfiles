[alias]
    # shortcut
	st = status -bs
	co = checkout
	br = branch
	ci = commit
	d = diff
	p = push
	a = add
    # manipulation
	unstage = reset HEAD --
	pa = push --all
	maj = pull --ff-only
	fa = fetch -pft
	ua = !git fa && git maj
    # view
	last = log -1 HEAD
	lop = log --graph --all --oneline --decorate
	lol = log --pretty=format:'%C(yellow)%h %Cred%ad %Cblue%an%Cgreen%d %Creset%s' --date=short
	dc = diff --cached
	dl = diff --name-only
[core]
	editor = vim
	excludesfile = ~/.config/git/ignore
	fileMode = false
